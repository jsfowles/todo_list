<h1>To Do(s)</h1>
<p><%= link_to 'New TODO(s) item', new_item_path%></p>
<% @items.each do |item| %>
  <h2><%= item.name %></h2>
  <p><%= item.description %></p>
  <%= link_to 'Show Entry', item_path(item)%>
  <%= link_to 'Edit', edit_item_path(item)%>
  <%= link_to 'Delete', item_path(item), method: :delete %>

<% end %>
